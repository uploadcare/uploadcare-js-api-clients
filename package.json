{
  "name": "@uploadcare/api-clients",
  "private": true,
  "type": "module",
  "version": "6.2.1",
  "license": "MIT",
  "workspaces": [
    "packages/api-client-utils",
    "packages/upload-client",
    "packages/rest-client",
    "packages/signed-uploads"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build:docs:upload-client": "typedoc --out docs/upload-client packages/upload-client/src/index.ts --readme packages/upload-client/README.md --options ./typedoc.json",
    "build:docs:rest-client": "typedoc --out docs/rest-client packages/rest-client/src/index.ts --readme packages/rest-client/README.md  --options ./typedoc.json",
    "build:docs:signed-uploads": "typedoc --out docs/signed-uploads packages/signed-uploads/src/index.ts --readme packages/signed-uploads/README.md  --options ./typedoc.json",
    "build:docs": "rimraf docs && run-s build:docs:* && touch docs/.nojekyll",
    "build": "npm run build --workspaces",
    "lint": "run-s eslint prettier",
    "lint:fix": "run-s eslint:fix prettier:fix",
    "eslint:fix": "eslint ./ --ext=ts,js --fix",
    "eslint": "eslint ./ --ext=ts,js",
    "prettier:fix": "prettier --write './**/*.{ts,js}'",
    "prettier": "prettier --check './**/*.{ts,js}'",
    "test": "npm run test --workspaces --if-present",
    "test:production": "npm run test:production --workspaces --if-present",
    "release:prepare": "shipjs prepare",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/jest": "27.0.0",
    "@types/node": "16.0.0",
    "@typescript-eslint/eslint-plugin": "5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "chalk": "4.1.2",
    "dotenv": "^16.0.1",
    "dts-bundle-generator": "6.12.0",
    "eslint": "8.18.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.1.3",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.0",
    "jest": "^28.1.0",
    "jest-extended": "^3.1.0",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.4",
    "prettier-config-standard": "5.0.0",
    "prettier-plugin-jsdoc": "^0.4.2",
    "rimraf": "3.0.2",
    "rollup": "^2.75.5",
    "rollup-plugin-node-externals": "^4.0.0",
    "shipjs": "0.24.0",
    "ts-jest": "28.0.3",
    "ts-node": "^10.8.1",
    "tslib": "^2.4.0",
    "typedoc": "^0.22.17",
    "typescript": "^4.7.4"
  }
}
